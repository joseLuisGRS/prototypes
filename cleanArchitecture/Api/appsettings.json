{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CustomLoggingLevel": "Debug",
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={SERVER},{PORT}\\{INSTANCE};Database={DB};User ID={USER};Password={PASSWORD};Connect Timeout=30;TrustServerCertificate=True;ApplicationIntent=ReadWrite;MultipleActiveResultSets=true;",
    "DBHost": "DB_HOST",
    "DBPort": "DB_PORT",
    "DBInstance": "DB_INSTANCE",
    "DBName": "DB_NAME",
    "DBUser": "DB_USER",
    "DBPassword": "DB_PASSWORD"
  },
  "RateLimiting": {
    "PermitLimit": 10,
    "Window": 20,
    "QueueLimit": 5
  },
  "DocSwagger": {
    "Contact": {
      "Name": "José Luis García R",
      "Email": "joseluis@outlook.com",
      "Url": "https://github.com/joseLuisGRS"
    },
    "InfoApi": {
      "Title": "Clean Architecture API",
      "Description": "Este proyecto es un prototipo y tiene la finalidad del uso de los estándares de los proyectos generados en .NET o tecnologías similares."
    }
  }
}
